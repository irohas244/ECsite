
public class Main {
	static String name = "すけさん";
	static int lv = 45;                         //プレイヤーのレベル

		public static void main(String[] args){
			putJyosyou();

			if(lv < 40){
				putGameOver();
			}else{
				putGameClear();
			}

		}

		static void putJyosyou(){
			System.out.println("魔王が" + "世界を滅ぼそうとしている");
			System.out.println();
		}
		static void putGameOver()

}


public class HelloWorld {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

		String a = "赤";
		
     switch(a){
     case "赤":
     System.out.println("赤組");
     break;
     
     case "白":
     System.out.println("白組");
     break;
     
     default:
    	 System.out.println("aは0でも1でもない");
     
     }
	}
}

